# AI API Keys
OPENAI_API_KEY=your_openai_key_here
GROQ_API_KEY=your_groq_key_here
OLLAMA_API_BASE=http://127.0.0.1:11434

# Web UI Settings
WEBUI_AUTH=False
ENABLE_IMAGE_GENERATION=True
IMAGE_GENERATION_MODEL=dall-e-3
ENABLE_RAG_WEB_SEARCH=True
RAG_WEB_SEARCH_ENGINE=duckduckgo
AUDIO_STT_ENGINE=openai
AUDIO_STT_MODEL=whisper-1
AUDIO_TTS_ENGINE=openai
AUDIO_TTS_VOICE=shimmer
ENABLE_EVALUATION_ARENA_MODELS=False

# System Paths
# Replace these with your desired paths
MODELS_PATH=/path/to/your/models
WEBUI_STATE_DIR=/path/to/your/webui/state

# Note: Copy this file to .env and replace the values with your actual configuration