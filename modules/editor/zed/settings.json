{
  "features": {
    "edit_prediction_provider": "zed"
  },
  "assistant": {
    "default_model": {
      "provider": "openai",
      "model": "gpt-4o"
    },
    "version": "2"
  },
  "ui_font_size": 16,
  "buffer_font_size": 16,
  "theme": {
    "mode": "system",
    "light": "Catppuccin Frapp√©",
    "dark": "One Dark"
  },
  "languages": {
    "Nix": {
      "language_servers": ["nixd"],
      "formatter": "alejandra",
      "format_on_save": "on"
    }
  },
  "lsp": {
    "nixd": {
      "binary": {
        "path": "/run/current-system/sw/bin/nixd"
      }
    }
  },
  "formatter": {
    "alejandra": {
      "command": "/run/current-system/sw/bin/alejandra",
      "arguments": ["--quiet"]
    }
  }
}
